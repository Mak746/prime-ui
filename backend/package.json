    "start:job:dev": "NODE_ENV=development nest start job --watch",
    "start:chat:dev": "NODE_ENV=development nest start chats --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/api/main",
